<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mainmenuholder"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <FrameLayout
        android:id="@+id/holder"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="bottom" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginBottom="20dp"
            android:orientation="vertical" >

            <Button
                android:id="@+id/button01"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/lighterback"
                android:gravity="left"
                android:text="The Council"
                android:textColor="#ffffff"
                android:textSize="23sp" />

            <Button
                android:id="@+id/button02"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/lighterback"
                android:gravity="left"
                android:text="Annual Conference"
                android:textColor="#ffffff"
                android:textSize="23sp" />

            <Button
                android:id="@+id/button03"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/lighterback"
                android:gravity="left"
                android:text="Workshop &amp; Seminars"
                android:textColor="#ffffff"
                android:textSize="23sp" />

            <Button
                android:id="@+id/button04"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/lighterback"
                android:gravity="left"
                android:text="Studies And Presentations"
                android:textColor="#ffffff"
                android:textSize="23sp" />

            <Button
                android:id="@+id/button05"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/lighterback"
                android:gravity="left"
                android:text="Members"
                android:textColor="#ffffff"
                android:textSize="23sp" />

            <Button
                android:id="@+id/button06"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/lighterback"
                android:gravity="left"
                android:text="Become a Member"
                android:textColor="#ffffff"
                android:textSize="23sp" />
        </LinearLayout>
    </FrameLayout>

</FrameLayout>
